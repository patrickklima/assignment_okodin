<div class="row">
  <div class="col-xs-3">
    <img src={{profile.picUrl}} alt={{user.username}} class="img-thumbnail">
  </div>
  <div class="col-xs-3">
    <h3>{{user.username}}</h3>
    <p>Location: {{profile.location}}</p>
  </div>
</div>
<hr>
<div class="row">
  <div class="col-xs-6">
    <h4>About Me</h4>
    <p>{{profile.aboutMe}}</p>
  </div>
  <div class="col-xs-3 col-xs-offset-3 active">
    <h4>Details</h4>
    <p>{{profile.age}}, {{profile.gender}}</p>
    <p>{{profile.height}} cm</p>
    {{#if profile.isSingle}}
      <p>Single</p>
    {{else}}
      <p>Not Single</p>
    {{/if}}
    {{#if profile.hasKids}}
      <p>Has kids</p>
    {{else}}
      <p>Has no kids</p>
    {{/if}}
  </div>
</div>
<div class="row">
  <div class="col-xs-6">
    <h4>Talents</h4>
    <ul class="list-inline">
      {{#each talents as |talent|}}
        <li>{{talent.dataValues.talentName}}</li>
      {{/each}}
    </ul>
  </div>
</div>
<div class="row">
  <div class="col-xs-6">
    <h4>Favorites</h4>
    <ul class="list-inline">
      {{#each favorites as |favorite|}}
        <li>{{favorite.dataValues.favoriteThing}}</li>
      {{/each}}
    </ul>
  </div>
</div>
    